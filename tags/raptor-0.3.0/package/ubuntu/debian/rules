#!/usr/bin/make -f
# debian/rules file - for raptor

include /usr/share/cdbs/1/rules/debhelper.mk
include /usr/share/cdbs/1/class/qmake.mk
QMAKE=qmake-qt4
CFLAGS=-O3
CXXFLAGS=-O3

install/raptor::
	rm -rf `find . -type d -name .svn`
	dh_install /delivery/release/raptor /usr/bin/
	dh_install package/ubuntu/applications /usr/share/
	dh_install package/ubuntu/menu /usr/share/
	dh_install package/ubuntu/pixmaps /usr/share/
